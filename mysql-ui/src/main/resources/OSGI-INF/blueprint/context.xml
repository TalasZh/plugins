<?xml version="1.0" encoding="UTF-8"?>
<blueprint default-activation="eager" xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

  <reference id="mysql" availability="mandatory" interface="org.safehaus.subutai.plugin.mysqlc.api.MySQLC" />
  <reference id="tracker" availability="mandatory" interface="org.safehaus.subutai.core.tracker.api.Tracker" />
  <reference id="environmentManager" availability="mandatory"
             interface="org.safehaus.subutai.core.env.api.EnvironmentManager" />

  <bean id="module" scope="singleton" class="org.safehaus.subutai.plugin.mysql.ui.MySQLPortalModule"
        init-method="init" destroy-method="destroy">
    <argument ref="mysql" />
    <argument ref="tracker" />
    <argument ref="environmentManager" />
  </bean>


  <service ref="module" interface="org.safehaus.subutai.server.ui.api.PortalModule" />

</blueprint>